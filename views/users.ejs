<!DOCTYPE html>
<html lang="en">
<body>    
    <h2>Total views <%= views %> </h2>
    This is users page
    <h1><%= title %></h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Industry </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
            <tr>
                <td><%= user.name %> </td>
                <td><%= user.industry %> </td>
                <td>
                    <button>View Details</button>
                    <button>Edit</button>
                </td>
            </tr>
            <% })  %>
        </tbody>
    </table>
    
    
</body>
</html>